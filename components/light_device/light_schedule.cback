static int sch_excel(Sch_file_t *p_sch){
	int ret = 0;
	int power_s = -1, bri_s = -1;
	float fade_s = -1;

	if( NULL == p_sch){
		MDF_LOGE("NULL sch \n");
		return -1;
		}
	// todo remove 
	_sch_file_printf(p_sch);
	
	if(p_sch->had.self){
