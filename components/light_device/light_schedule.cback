		sprintf(p_key_str, "%s_%d",_p_sch_key[stype], index );
		mdf_info_save( p_key_str,  p_sub, sizeof( Item_sub_t ) + p_sub->name_len );

		MDF_LOGD("Add sub item %s to key %s \n", p_sub->p_name, p_key_str);
